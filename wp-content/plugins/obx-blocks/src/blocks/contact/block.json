{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "obx-blocks/contact",
  "version": "1.0.0",
  "title": "Contact",
  "category": "obx-blocks",
  "icon": "email",
  "description": "Contact form block with background image and content",
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "anchor": true
  },
  "attributes": {
    "anchor": {
      "type": "string"
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "styledTitle": {
      "type": "string",
      "default": ""
    },
    "text": {
      "type": "string",
      "default": ""
    },
    "email": {
      "type": "string",
      "default": ""
    },
    "phone": {
      "type": "string",
      "default": ""
    },
    "address": {
      "type": "string",
      "default": ""
    },
    "formShortcode": {
      "type": "string",
      "default": ""
    },
    "backgroundImageUrl": {
      "type": "string",
      "default": ""
    },
    "backgroundImageId": {
      "type": "number",
      "default": null
    },
    "backgroundImageAlt": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "textColor": {
      "type": "string",
      "default": "#000000"
    },
    "accentColor": {
      "type": "string",
      "default": "#C4A468"
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "textAlign": {
      "type": "string",
      "default": "left"
    },
    "contentWidth": {
      "type": "number",
      "default": 100
    }
  },
  "textdomain": "obx-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
} 